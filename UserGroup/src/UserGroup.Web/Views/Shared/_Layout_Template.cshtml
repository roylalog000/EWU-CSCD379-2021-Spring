<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>UserGroup - @ViewBag.Title</title>
    <!-- Stylesheets -->
    <% for (var filename in htmlWebpackPlugin.files.css) { %>
    <link href="~<%= htmlWebpackPlugin.files.css[filename] %>" rel="stylesheet" />
    <% } %>
    <!-- End Stylesheets -->
</head>
<body>
    <nav class="bg-green-500" >
        <ul class="flex text-white">
            <li><a class="inline-block p-6 leading-10 text-xl" asp-controller="Home" asp-action="Index">Home</a></li>
            <li><a class="inline-block p-6 leading-10" asp-controller="Speakers" asp-action="Index">Speakers</a></li>
            <li><a class="inline-block p-6 leading-10" asp-controller="Events" asp-action="Index">Events</a></li>
        </ul>
    </nav>
    @RenderBody()

    <!-- Scripts -->
    <% for (var index in htmlWebpackPlugin.files.js) { %>
    <script src="~<%= htmlWebpackPlugin.files.js[index] %>"></script>
    <% } %>
    <!-- End Scripts -->
</body>
</html>